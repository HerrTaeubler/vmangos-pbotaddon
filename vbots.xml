<Ui xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.blizzard.com/wow/ui/">
	<Script file="Frame.lua" />
	<Frame name="vbotsFrame" parent="UIParent" toplevel="true" enableMouse="true">
		<Size x="770" y="120" />
		<Anchors>
			<Anchor point="BOTTOMRIGHT">
				<Offset>
					<AbsDimension x="0" y="90"/>
				</Offset>
			</Anchor>
		</Anchors>

		<Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background" edgeFile="Interface\DialogFrame\UI-DialogBox-Border" tile="true">
			<BackgroundInsets>
				<AbsInset left="11" right="12" top="12" bottom="11" />
			</BackgroundInsets>
			<TileSize>
				<AbsValue val="32" />
			</TileSize>
			<EdgeSize>
				<AbsValue val="32" />
			</EdgeSize>
		</Backdrop>
		<!-- overlay labels layer for below buttons frame-->
        <Layers>
			<Layer level="OVERLAY">
			    <!-- upper left -->
				<FontString name="AddBot" inherits="GameFontNormal" text="vmangos bot command">
						<Size x="220" y="20" />
						<Anchors>
							<Anchor point="TOPLEFT">
								<Offset>
									<AbsDimension x="0" y="-2"/>
								</Offset>
							</Anchor>
						</Anchors>
				</FontString>
			
			</Layer>
		</Layers>
		<!-- buttons frame -->
		<Frames>
		    <!-- left 1 column -->
			<Button name="PartyBotClone" inherits="UIPanelButtonTemplate" text="Clone Bot">
				<Size x="100" y="30" />
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset>
							<AbsDimension x="10" y="-20"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>
						SubPartyBotClone(self);
					</OnClick>
				</Scripts>
			</Button>
			
			<Button name="PartyBotRemove" inherits="UIPanelButtonTemplate" text="Remove Bot">
				<Size x="100" y="30" />
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset>
							<AbsDimension x="10" y="-50"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>
						SubPartyBotRemove(self);
					</OnClick>
				</Scripts>
			</Button>
			

			<!-- left 2 column -->
			<Button name="PartyBotAdd" inherits="UIPanelButtonTemplate" text="Add Warrior">
				<Size x="100" y="30" />
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset>
							<AbsDimension x="120" y="-20"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>
						SubPartyBotAdd(self, "warrior");
					</OnClick>
				</Scripts>
			</Button>
	        
			
			<!-- right 1 column -->
			<Button name="BattleBotAddwarsonghorde" inherits="UIPanelButtonTemplate" text="warsong horde">
				<Size x="100" y="30" />
				<Anchors>
					<Anchor point="TOPRIGHT">
						<Offset>
							<AbsDimension x="-10" y="-20"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>
						SubBattleBotAdd(self, "warsong", "horde");
					</OnClick>
				</Scripts>
			</Button>
			
			<Button name="BattleBotAddarathihorde" inherits="UIPanelButtonTemplate" text="arathi horde">
				<Size x="100" y="30" />
				<Anchors>
					<Anchor point="TOPRIGHT">
						<Offset>
							<AbsDimension x="-10" y="-50"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>
						SubBattleBotAdd(self, "arathi", "horde");
					</OnClick>
				</Scripts>
			</Button>
			
			<Button name="BattleBotAddalterachorde" inherits="UIPanelButtonTemplate" text="alterac horde">
				<Size x="100" y="30" />
				<Anchors>
					<Anchor point="TOPRIGHT">
						<Offset>
							<AbsDimension x="-10" y="-80"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>
						SubBattleBotAdd(self, "alterac", "horde");
					</OnClick>
				</Scripts>
			</Button>
			
			
			<!-- right 2 column -->
			<Button name="BattleBotAddwarsonghorde" inherits="UIPanelButtonTemplate" text="warsong alliance">
				<Size x="100" y="30" />
				<Anchors>
					<Anchor point="TOPRIGHT">
						<Offset>
							<AbsDimension x="-120" y="-20"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>
						SubBattleBotAdd(self, "warsong", "alliance");
					</OnClick>
				</Scripts>
			</Button>
			
			<Button name="BattleBotAddarathihorde" inherits="UIPanelButtonTemplate" text="arathi alliance">
				<Size x="100" y="30" />
				<Anchors>
					<Anchor point="TOPRIGHT">
						<Offset>
							<AbsDimension x="-120" y="-50"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>
						SubBattleBotAdd(self, "arathi", "alliance");
					</OnClick>
				</Scripts>
			</Button>
			
			<Button name="BattleBotAddalterachorde" inherits="UIPanelButtonTemplate" text="alterac alliance">
				<Size x="100" y="30" />
				<Anchors>
					<Anchor point="TOPRIGHT">
						<Offset>
							<AbsDimension x="-120" y="-80"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>
						SubBattleBotAdd(self, "alterac", "alliance");
					</OnClick>
				</Scripts>
			</Button>
			
			<!-- right 3 column -->
			<Button name="BattleGowarsong" inherits="UIPanelButtonTemplate" text="go warsong">
				<Size x="100" y="30" />
				<Anchors>
					<Anchor point="TOPRIGHT">
						<Offset>
							<AbsDimension x="-230" y="-20"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>
						SubBattleGo(self, "warsong");
					</OnClick>
				</Scripts>
			</Button>
			
			<Button name="BattleBotAddarathihorde" inherits="UIPanelButtonTemplate" text="go arathi">
				<Size x="100" y="30" />
				<Anchors>
					<Anchor point="TOPRIGHT">
						<Offset>
							<AbsDimension x="-230" y="-50"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>
						SubBattleGo(self, "arathi");
					</OnClick>
				</Scripts>
			</Button>
			
			<Button name="BattleBotAddalterachorde" inherits="UIPanelButtonTemplate" text="go alterac">
				<Size x="100" y="30" />
				<Anchors>
					<Anchor point="TOPRIGHT">
						<Offset>
							<AbsDimension x="-230" y="-80"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>
						SubBattleGo(self, "alterac");
					</OnClick>
				</Scripts>
			</Button>
			
        	<Button name="CloseButton" inherits="UIPanelButtonTemplate" text="Close">
				<Size x="120" y="30" />
				<Anchors>
					<Anchor point="BOTTOM">
						<Offset>
							<AbsDimension x="0" y="0"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>
						CloseFrame();
					</OnClick>
				</Scripts>
			</Button>
		</Frames>
	</Frame>
	<Button name="AttributeOptions" inherits="GameMenuButtonTemplate" parent="GameMenuFrame" text="VBOTS">
		<Anchors>
			<Anchor point="CENTER" relativePoint="BOTTOM">>
				<Offset>
					<AbsDimension x="0" y="10"/>
				</Offset>
			</Anchor>
		</Anchors>
		<Scripts>
			<OnClick>
				OpenFrame();
			</OnClick>
		</Scripts>
	</Button>
</Ui>